<!-- ALQR	AliPay QR	Global -->
    <record id="payment_method_alipay_qr" model="payment.method">
        <field name="name">Alipay QR</field>
        <field name="code">ALQR</field>
        <field name="image" type="base64" file="payment/static/img/alipay_qr.png"/>
        <field name="sequence">1000</field>
        <field name="active">False</field>
    </record>

