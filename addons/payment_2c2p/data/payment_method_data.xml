<!-- SHPQR	Shopeepay Wallet (Customer QR)	SG / MY / ID / VN -->
    <record id="payment_method_shopeepay_qr" model="payment.method">
        <field name="name">Shopeepay Wallet (Customer QR)</field>
        <field name="code">SHPQR</field>
        <field name="image" type="base64" file="payment/static/img/shopeepay_qr.png"/>
        <field name="sequence">1000</field>
        <field name="active">False</field>
        <field name="supported_country_ids" eval="[Command.set([
            ref('base.sg'),
            ref('base.my'),
            ref('base.id'),
            ref('base.vn'),
        ])]"/>
        <field name="supported_currency_ids" eval="[Command.set([
            ref('base.SGD'),
            ref('base.MYR'),
            ref('base.IDR'),
            ref('base.VND'),
        ])]"/>
    </record>

