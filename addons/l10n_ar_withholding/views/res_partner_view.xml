<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record model="ir.ui.view" id="view_partner_form">
        <field name="name">res.partner.form.inherit</field>
        <field name="inherit_id" ref="account.view_partner_property_form"/>
        <field name="model">res.partner</field>
        <field name="arch" type="xml">
            <page name="sales_purchases" position="after">
                <page string="Ar Taxes" name="accounting">
                    <field name="arba_alicuot_ids">
                        <tree editable="top">
                            <!-- <field name="company_id" groups="base.group_multi_company" options="{'no_create': True, 'no_open': True}"/> -->
                            <field name="tax_id" options="{'no_create': True, 'no_open': True}"/>
                            <field name="from_date"/>
                            <field name="to_date"/>
                            <field name="ref"/>
                        </tree>
                    </field>
                </page>
            </page>
        </field>
    </record>

</odoo>
