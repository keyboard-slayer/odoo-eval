<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="account_move_form_inherited" model="ir.ui.view">
        <field name="name">account.move.form.inherited</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//form[1]/sheet[1]/notebook[1]/page[@name='invoice_tab']/field[@name='invoice_line_ids']/tree[1]/field[@name='price_unit']" position="attributes">
                <attribute name="string">List Price</attribute>
              </xpath>
              <xpath expr="//form[1]/sheet[1]/notebook[1]/page[@name='invoice_tab']/field[@name='invoice_line_ids']/tree[1]/field[@name='tax_ids']" position="before">
                <field name="discounted_price_invoice" optional="show"/>
              </xpath>
        </field>
    </record>
</odoo>