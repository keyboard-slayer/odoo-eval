<?xml version="1.0" encoding="UTF-8"?>

<templates xml:space="preserve">
    <t t-name="l10n_in_tds_entry.AccountPaymentField" t-inherit="account.AccountPaymentField" t-inherit-mode="extension" owl="1">
        <xpath expr="//i[hasclass('o_payment_label')]" position="replace">
            <i class="o_field_widget text-start o_payment_label">
                <t t-if="line.l10n_in_is_tds">TDS on</t><t t-else="">Paid on</t> <t t-out="line.date"></t>
            </i>
        </xpath>
    </t>
</templates>
