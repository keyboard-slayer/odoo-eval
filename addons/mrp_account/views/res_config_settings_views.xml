<odoo>
    <data>
        <record id="res_config_settings_view_form" model="ir.ui.view">
            <field name="name">res.config.settings.view.form.inherit.mrp.account</field>
            <field name="model">res.config.settings</field>
            <field name="inherit_id" ref="mrp.res_config_settings_view_form" />
            <field name="arch" type="xml">
                <xpath expr="//setting[@id='mrp_reception_report']" position="after">
                    <setting id="mrp_wip" help="Record intermediary consumption of components, overheads and labour">
                        <field name="group_mrp_wip"/>
                    </setting>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
