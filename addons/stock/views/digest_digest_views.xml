<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <record id="digest_digest_view_form" model="ir.ui.view">
        <field name="name">digest.digest.view.form.inherit.stock</field>
        <field name="model">digest.digest</field>
        <field name="priority">20</field>
        <field name="inherit_id" ref="digest.digest_digest_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='kpis']/group[last()]" position="before">
                <group name="kpi_stock" string="Stock" groups="stock.group_stock_manager">
                    <field name="kpi_stock_delivery_count"/>
                    <field name="kpi_stock_receipt_count"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>
