<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">
    <t t-name="BomOverviewComponentsBlock" t-inherit="mrp.BomOverviewComponentsBlock" t-inherit-mode="extension">
        <xpath expr="//t[@name='byproducts']" position="after">
            <t name="services" t-if="!!data.services &amp;&amp; data.services.length > 0">
                <BomOverviewExtraBlock
                    unfoldAll="state.unfoldAll"
                    type="'services'"
                    showOptions="props.showOptions"
                    data="data"
                    precision="props.precision"
                    changeFolded.bind="props.changeFolded"/>
            </t>
        </xpath>
    </t>
</templates>
