<templates xml:space="preserve">
    <t t-name="l10n_in_withholding.AccountPaymentField" t-inherit="account.AccountPaymentField" t-inherit-mode="extension">
        <xpath expr="//td[@t-if='!line.is_exchange']" position="replace">
            <td t-if="line.l10n_in_is_withholding">
                <i class="o_field_widget text-start o_payment_label">Tds on <t t-out="line.date"></t></i>
            </td>
            <t t-else="">
                <td t-if="!line.is_exchange">
                    <i class="o_field_widget text-start o_payment_label">Paid on <t t-out="line.date"></t></i>
                </td>
            </t>
        </xpath>
    </t>
</templates>
