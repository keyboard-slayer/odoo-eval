<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="stock_move_line_tree_inherited" model="ir.ui.view">
        <field name="name">stock.move.line.tree.inherited</field>
        <field name="model">stock.move.line</field>
        <field name="inherit_id" ref="stock.view_stock_move_line_detailed_operation_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='quant_id']" position="after">
                <field name="x_warranty_terms_stock"/>
            </xpath>
            <xpath expr="//field[@name='lot_id']" position="after">
                <field name="x_warranty_expire"/>
            </xpath>
        </field>
    </record>
</odoo>
