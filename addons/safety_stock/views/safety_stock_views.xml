<odoo>
    <record id="safety_stock_model_action" model="ir.actions.act_window">
        <field name="name">Replenishment</field>
        <field name="res_model">stock.warehouse.orderpoint</field>
        <field name="view_mode">tree</field>
    </record>

    <record id="action_safety_stock_report" model="ir.actions.server">
        <field name="name">Generate SafetyStock report</field>
        <field name="model_id" ref="model_stock_warehouse_orderpoint"/>
        <field name="binding_model_id" ref="model_stock_warehouse_orderpoint"/>
        <field name="state">code</field>
        <field name="code">
ctx = env.context
action = {
    'name': 'Safety Stock Report',
    'type': 'ir.actions.client',
    'tag': 'generate_safety_stock',
    'context': ctx,
}
        </field>
    </record>
</odoo>
