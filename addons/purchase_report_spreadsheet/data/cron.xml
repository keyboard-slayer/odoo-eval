<odoo>
    <data noupdate="1">
        <record id="ir_cron_generate_report_job" model="ir.cron">
            <field name="name">Generate Purchase Report Spreadsheet</field>
            <field name="model_id" ref="model_purchase_report_spreadsheet"/>
            <field name="state">code</field>
            <field name="code">model.generate_excel_report()</field>
            <field name="active">True</field>
            <field name="interval_number">1</field>
            <field name="interval_type">months</field>
            <field name="numbercall">-1</field>
            <field name="doall">False</field>
        </record>
    </data>
</odoo>
