<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="account_move_form_inherited" model="ir.ui.view">
        <field name="name">account.move.form.inherited</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='price_unit']" position="replace">
                <field name="x_unit_price"/>
            </xpath>
            <xpath expr="//field[@name='tax_ids']" position="after">
                <field name="x_first_discount"/>
                <field name="x_second_discount"/>
                <field name="x_third_discount"/>
                <field name="x_fourth_discount"/>
                <field name="x_final_price"/>
            </xpath>
        </field>
    </record>
</odoo>
