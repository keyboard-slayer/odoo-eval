<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="l10n_ro_edi_etransport_report_delivery_document" inherit_id="stock.report_delivery_document" priority="100">
        <xpath expr="//div/strong[text()='Tracking Number:']/.. | //div/strong[text()='Total Weight:']/.." position="after">
            <div t-if="l10n_ro_edi_etransport_should_show and o.l10n_ro_edi_etransport_uit" class="col-auto" name="div_etransport_uit">
                <strong>eTransport UIT:</strong>
                <p t-field="o.l10n_ro_edi_etransport_uit"/>
            </div>
        </xpath>
    </template>
</odoo>
