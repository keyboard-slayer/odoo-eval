<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <t t-name="project.project_sharing_control_panel" t-inherit="web.ControlPanel" t-inherit-mode="primary">
        <xpath expr="//div[hasclass('o_control_panel_breadcrumbs')]/t[@t-else='']" position="replace">
            <t t-if="!env.isSmall">
                <ProjectSharingBreadcrumbs breadcrumbs="breadcrumbs">
                    <t t-set-slot="breadcrumb-status-indicator">
                        <t t-slot="control-panel-status-indicator" />
                    </t>
                    <t t-set-slot="breadcrumb-additional-actions">
                        <t t-slot="control-panel-additional-actions"/>
                    </t>
                </ProjectSharingBreadcrumbs>
            </t>
        </xpath>
    </t>

</templates>
