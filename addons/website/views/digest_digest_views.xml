<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <record id="digest_digest_view_form" model="ir.ui.view">
        <field name="name">digest.digest.view.form.inherit.website</field>
        <field name="model">digest.digest</field>
        <field name="priority">20</field>
        <field name="inherit_id" ref="digest.digest_digest_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='kpis']/group[last()]" position="before">
                <group name="kpi_website" string="Website" groups="website.group_website_restricted_editor">
                    <field name="kpi_website_visitor_count"/>
                    <field name="kpi_website_track_count"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>
