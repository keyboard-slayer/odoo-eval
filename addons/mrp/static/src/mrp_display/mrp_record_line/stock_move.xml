<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">
    <t t-name="mrp.StockMove" owl="1">
        <li class="o_mrp_record_line list-group-item py-3"
            t-att-class="cssClass"
            t-on-click="onClick"
            t-on-animationend="onAnimationEnd">
            <span class="me-2">
                <t t-esc="props.record.data.quantity_done"/>
                <t t-if="props.record.data.product_uom_qty"> / <t t-esc="props.record.data.product_uom_qty"/></t>
            </span>
            <span t-esc="props.record.data.product_id[1]"/>
        </li>
    </t>
</templates>
