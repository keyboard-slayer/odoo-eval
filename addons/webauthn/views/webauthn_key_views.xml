<?xml version="1.0"?>
<odoo>
    <record id="webauthn_key_views_field" model="ir.ui.view">
        <field name="name">webauthn.key.views.field</field>
        <field name="model">webauthn.key</field>
        <field name="arch" type="xml">
            <form js_class="webauthn_insert_creds_form">
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="credential_identifier" invisible="true"/>
                        <field name="public_key" invisible="true"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="webauthn_key_views_tree" model="ir.ui.view">
        <field name="name">webauthn.key.views.tree</field>
        <field name="model">webauthn.key</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>
</odoo>
