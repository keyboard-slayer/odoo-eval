<?xml version="1.0"?>
<odoo>
    <record id="view_cloud_storage_blob_to_delete_tree" model="ir.ui.view">
        <field name="name">cloud.storage.blob.to.delete.tree</field>
        <field name="model">cloud.storage.blob.to.delete</field>
        <field name="arch" type="xml">
            <tree string="Cloud Storage To Delete Blobs">
                <header>
                    <button string="Delete Cloud Storage Blobs" name="delete_blobs" type="object"/>
                </header>
                <field name="url"/>
                <field name="provider_model"/>
                <field name="state"/>
                <field name="error_message"/>
            </tree>
        </field>
    </record>

    <record id="action_cloud_storage_blob_to_delete" model="ir.actions.act_window">
        <field name="name">Cloud Storage Blobs To Delete</field>
        <field name="res_model">cloud.storage.blob.to.delete</field>
        <field name="view_mode">tree</field>
        <field name="view_id" ref="view_cloud_storage_blob_to_delete_tree"/>
    </record>

</odoo>
