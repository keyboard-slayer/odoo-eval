<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="demo_bank_no" model="res.partner.bank">
        <field name="acc_number">NO8330001234567</field>
        <field name="partner_id" model="res.partner" eval="obj().env.ref('base.demo_company_no').partner_id"/>
        <field name="company_id" ref="base.demo_company_no"/>
    </record>

    <record id="base.demo_company_no" model="res.company">
        <field name="l10n_no_bronnoysund_number">123456785</field>
        <field name="active" eval="True"/>
    </record>

    <function model="account.chart.template" name="try_loading">
        <value eval="[]"/>
        <value>no</value>
        <value model="res.company" eval="obj().env.ref('base.demo_company_no')"/>
    </function>
</odoo>
