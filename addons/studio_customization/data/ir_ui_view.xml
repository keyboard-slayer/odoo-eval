  <record id="studio_customization.odoo_studio_analytic_7b34f445-4598-4c2c-80e7-37ccf5bb28d3" model="ir.ui.view" context="{'studio': True}">
    <field name="active" eval="True"/>
    <field name="arch" type="xml">
      <data>
        <xpath expr="//field[@name='code']" position="replace">
          <field widget="many2many_tags" name="x_studio_building_1" options="{&quot;color_field&quot;: &quot;x_color&quot;}" string="Building"/>
          <field name="x_studio_address" string="Address"/>
        </xpath>
        <xpath expr="//button[@name='138']" position="replace"/>
        <xpath expr="//field[@name='invoice_count']" position="attributes">
          <attribute name="string">Invoices</attribute>
        </xpath>
        <xpath expr="//field[@name='subscription_count']" position="attributes">
          <attribute name="string">Rental contracts</attribute>
        </xpath>
        <xpath expr="//widget[@name='web_ribbon']" position="after">
          <field name="x_studio_add_pictures_of_the_property" widget="image" class="oe_avatar ml-3 mr-3" options="{&quot;size&quot;:[0,180]}"/>
        </xpath>
        <xpath expr="//form[1]/sheet[1]/div[not(@name)][1]/label[1]" position="attributes">
          <attribute name="string">Property</attribute>
        </xpath>
        <xpath expr="//field[@name='partner_id']" position="attributes">
          <attribute name="attrs">{}</attribute>
          <attribute name="invisible">1</attribute>
        </xpath>
        <xpath expr="//field[@name='plan_id']" position="attributes">
