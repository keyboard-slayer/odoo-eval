<?xml version='1.0' encoding='UTF-8'?>
<odoo>
  <record id="sale_subscription.sale_subscription_action" model="ir.actions.act_window" context="{'studio': True}">
    <field name="binding_model_id" eval="False"/>
    <field name="binding_type">action</field>
    <field name="binding_view_types">list,form</field>
    <field name="context">{
            'default_is_subscription': 1
        }</field>
    <field name="domain">[('is_subscription', '=', True)]</field>
    <field name="filter" eval="False"/>
    <field name="groups_id" eval="[(6, 0, [])]"/>
    <field name="help"><![CDATA[<p class="o_view_nocontent_smiling_face">
                Create a new subscription
            </p><p>
                Create subscriptions to manage recurring invoicing and payments. Subscriptions can
                be time-bounded or not. In case of a limited period, they are flagged as to be renewed
                one month from the end date.
            </p><p>
                Subscriptions can be automatically generated from sales orders in Sales or eCommerce
                apps.
            </p>
        ]]></field>
    <field name="limit">80</field>
    <field name="name">Subscriptions</field>
    <field name="res_model">sale.order</field>
    <field name="search_view_id" ref="sale_subscription.sale_subscription_view_search"/>
    <field name="target">current</field>
    <field name="type">ir.actions.act_window</field>
    <field name="usage" eval="False"/>
    <field name="view_id" eval="False"/>
    <field name="view_mode">kanban,tree,form,pivot,graph,cohort,activity,gantt</field>
  </record>
