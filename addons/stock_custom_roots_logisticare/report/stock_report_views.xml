<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
    <!--paper formats-->
    <record id="paperformat_1" model="report.paperformat">
        <field name="name">4" x 3" paper</field>
        <field name="default" eval="True" />
        <field name="format">custom</field>
        <field name="page_height">77</field>
        <field name="page_width">102</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">5</field>
        <field name="margin_bottom">5</field>
        <field name="margin_left">3</field>
        <field name="margin_right">3</field>
        <field name="header_line" eval="False" />
        <field name="dpi">90</field>
    </record>
    <record id="paperformat_2" model="report.paperformat">
        <field name="name">3" x 1" paper</field>
        <field name="default" eval="True" />
        <field name="format">custom</field>
        <field name="page_height">77</field>
        <field name="page_width">26</field>
        <field name="orientation">Landscape</field>
        <field name="margin_top">1</field>
        <field name="margin_bottom">1</field>
        <field name="margin_left">1</field>
        <field name="margin_right">1</field>
        <field name="header_line" eval="False" />
        <field name="dpi">90</field>
    </record>

    <!--reports-->
    <record id="action_report_location_barcode_1" model="ir.actions.report">
        <field name="name">Location Barcode (4" x 3")</field>
        <field name="model">stock.location</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">stock_custom_roots_logisticare.report_location_barcode_1</field>
        <field name="report_file">stock_custom_roots_logisticare.report_location_barcode_1</field>
        <field name="paperformat_id" ref="stock_custom_roots_logisticare.paperformat_1"/>
        <field name="print_report_name">'Location - %s' % object.name</field>
        <field name="binding_model_id" ref="stock.model_stock_location"/>
        <field name="binding_type">report</field>
    </record>
    <record id="action_report_location_barcode_2" model="ir.actions.report">
        <field name="name">Location Barcode (3" x 1")</field>
        <field name="model">stock.location</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">stock_custom_roots_logisticare.report_location_barcode_2</field>
        <field name="report_file">stock_custom_roots_logisticare.report_location_barcode_2</field>
        <field name="paperformat_id" ref="stock_custom_roots_logisticare.paperformat_2"/>
        <field name="print_report_name">'Location - %s' % object.name</field>
        <field name="binding_model_id" ref="stock.model_stock_location"/>
        <field name="binding_type">report</field>
    </record>
    <record id="action_report_lot_label_1" model="ir.actions.report">
        <field name="name">Lot/Serial Number (PDF) (3" x 1")</field>
        <field name="model">stock.lot</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">stock_custom_roots_logisticare.report_lot_label_1</field>
        <field name="report_file">stock.report_lot_label_1</field>
        <field name="paperformat_id" ref="stock_custom_roots_logisticare.paperformat_2"/>
        <field name="print_report_name">'Lot-Serial - %s' % object.name</field>
        <field name="binding_model_id" ref="stock.model_stock_lot"/>
        <field name="binding_type">report</field>
    </record>
    <record id="action_report_product_1" model="ir.actions.report">
        <field name="name">Product wihtout price (4" x 3")</field>
        <field name="model">product.template</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">stock_custom_roots_logisticare.report_product</field>
        <field name="report_file">stock_custom_roots_logisticare.report_product</field>
        <field name="paperformat_id" ref="stock_custom_roots_logisticare.paperformat_1"/>
        <field name="print_report_name">'Products Labels - %s' % object.name</field>
        <field name="binding_model_id" ref="product.model_product_template"/>
        <field name="binding_type">report</field>
    </record>
    <record id="action_report_product_2" model="ir.actions.report">
        <field name="name">Product wihtout price (3" x 1")</field>
        <field name="model">product.template</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">stock_custom_roots_logisticare.report_product_second</field>
        <field name="report_file">stock_custom_roots_logisticare.report_product_second</field>
        <field name="paperformat_id" ref="stock_custom_roots_logisticare.paperformat_2"/>
        <field name="print_report_name">'Products Labels - %s' % object.name</field>
        <field name="binding_model_id" ref="product.model_product_template"/>
        <field name="binding_type">report</field>
    </record>
    </data>
</odoo>
