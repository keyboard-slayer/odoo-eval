    <!-- View -->
    <record id="fitter_expandable_register_view_tree" model="ir.ui.view">
        <field name="name">fitter_expandable.register.tree</field>
        <field name="model">fitter_expandable.register</field>
        <field name="arch" type="xml">
            <tree editable="top" create="1" delete="1" string="Channel" default_order="name desc">
                <field name ="name"/>
                <field name="referance_no"/>
                <field name="date_of_receipt"/>
                <field name="opening_balance"/>
                <field name="received_quantity"/>
                <field name="total_quantity"/>
                <field name="rate"/>
                <field name="sign_of_storekeeper_1" optional="1"/>
                <field name="signature_head_1"  optional="1"/>
                <field name="date_of_despatch"/>
                <field name="indent_no"/>
                <field name="indent_date"/>
                <field name="in_which_section_given"/>
                <field name="quantity_issued"/>
                <field name="sign_of_receiving_employee" optional="1"/>
                <field name="closing_balance"/>
                <field name="sign_of_storekeeper_2" optional="1"/>
                <field name="sign_of_principal" optional="1"/>
                <field name="remark"/>
            </tree>
        </field>
    </record>

