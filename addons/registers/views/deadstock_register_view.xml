    <!-- View -->
    <record id="deadstock_register_view_tree" model="ir.ui.view">
        <field name="name">deadstock.register.tree</field>
        <field name="model">deadstock.register</field>
        <field name="arch" type="xml">
            <tree editable="top" create="1" delete="1" string="Channel" default_order="name desc">
                <field name ="name"/>
                <field name="description_of_item"/>
                <field name="detail_of_purchase"/>
                <field name="details"/>
                <field name="price"/>
                <field name="sign_of_storekeeper_1"  optional="1"/>
                <field name="sign_of_HOD"  optional="1"/>
                <field name="date_of_issue"/>
                <field name="indent_no"/>
                <field name="indent_date"/>
                <field name="quantity_issued"/>
                <field name="sign_of_receiving_employee"  optional="1"/>
                <field name="closing_balance"/>
                <field name="sign_of_storekeeper_2"  optional="1"/>
                <field name="sign_of_principal_2"  optional="1"/>
                <field name="remark"/>
            </tree>
        </field>
    </record>

